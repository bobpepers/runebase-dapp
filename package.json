{
  "name": "tipbots-website",
  "version": "0.0.2",
  "description": "Website for Tipbots",
  "scripts": {
    "lingui:extract": "lingui extract",
    "lingui:compile": "lingui compile",
    "dev": "webpack serve --mode development --env development --config ./webpack/webpack-dev.config.js --color",
    "build": "rm -rf dist && node --max-old-space-size=8192 node_modules/webpack/bin/webpack.js --config ./webpack/webpack-prod.config.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src test webpack",
    "lint:fix": "eslint src test webpack --fix --ext .js,.jsx .",
    "fix-js": "eslint-auto-fix",
    "standard-version": "git add . && standard-version --commit-all",
    "pre-release:minor": "git add . && standard-version --skip.commit --skip.tag --release-as minor",
    "pre-release:patch": "git add . && standard-version --skip.commit --skip.tag --release-as patch",
    "pre-release:major": "git add . && standard-version --skip.commit --skip.tag --release-as major",
    "release-final": "git add . && standard-version --commit-all --skip.bump",
    "release": "npm-run-all build standard-version",
    "release:patch": "npm-run-all pre-release:patch build release-final",
    "release:minor": "npm-run-all pre-release:minor build release-final",
    "release:major": "npm-run-all pre-release:major build release-final"
  },
  "keywords": [
    "Runes",
    "Runebase",
    "Tipbot",
    "RunesTip",
    "PirateTip",
    "TokelTip",
    "Discord",
    "Telegram",
    "Matrix",
    "Website"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bobpepers/Tipbots-Website"
  },
  "author": "Jonas Collier",
  "devDependencies": {
